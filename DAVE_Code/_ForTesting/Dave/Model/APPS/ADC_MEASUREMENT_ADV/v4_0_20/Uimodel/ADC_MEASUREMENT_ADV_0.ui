                        <p1:GFloat description="This is the actual sample time value (non-editable). This is the duration for which the ADC samples the input signal." enabled="false" format="*,3" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="10000" minValue="0" widgetName="gstring_actual_sampleT" x:Style="BORDER" bounds="227, 228, 128, 21"/>
                        <p1:GFloat description="Shows the total conversion time, the configuration is done in the required source APP (non-editable)." enabled="false" format="*,3" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="POSITIVE_INFINITY" minValue="0" widgetName="gstring_coversion_time" x:Style="BORDER" bounds="227, 271, 127, 21"/>
                        <p1:GString x:Style="BORDER" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" widgetName="gstring_trigger_edge_sel" enabled="false" value="Falling Edge" bounds="90, 180, 76, 21" description="Shows the trigger configuration, the configuration is done in the required source APP (non-editable).&#13;&#10;&lt;UL&gt;&#13;&#10;&lt;LI&gt;No Trigger: To ignore the Trigger signal. &lt;/LI&gt;&#13;&#10;&lt;LI&gt;Rising Edge : To trigger the scan / queue conversion on a rising edge.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Falling Edge :  To trigger the scan / queue conversion on a falling edge .&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Both Edges: To trigger the scan / queue conversion on both rising and falling edges.&lt;/LI&gt;&#13;&#10;&lt;/ul&gt;"/>
                        <p1:GString enabled="false" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" value="Always convert" widgetName="gstring_gating_sel" x:Style="BORDER" bounds="90, 245, 103, 21" description="Shows the gating configuration, the configuration is done in the required source APP (non-editable).&#13;&#10;&lt;UL&gt;&#13;&#10;&lt;LI&gt; Block conversion: All conversion requests are blocked.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Always convert: All conversion requests are issued if there are valid entries present in the scan / queue.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Ungate on a high: Conversion requests are issued only when gate level is high.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Ungate on a low: Conversion requests are issued only when gate level is low.&lt;/LI&gt;&#13;&#10;&lt;/ul&gt;"/>
                        <p1:GString enabled="false" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" value="10 Bit Fast Conversion" widgetName="gstring_conv_mode" x:Style="BORDER" bounds="227, 182, 129, 21" description="Shows the actual result resolution, the configuration is done in the required source APP (non-editable).&#13;&#10;&lt;UL&gt;&#13;&#10;&lt;LI&gt;12 Bit Conversion: The result of the conversion will have a resolution of 12 bits. &lt;/LI&gt;&#13;&#10;&lt;LI&gt;10 Bit Conversion: The result of the conversion will have a resolution of 10 bits. &lt;/LI&gt;&#13;&#10;&lt;LI&gt;8 Bit Conversion: The result of the conversion will have a resolution of 8 bits. &lt;/LI&gt;&#13;&#10;&lt;LI&gt;10 Bit Fast Conversion: This mode will raise only one bit in the result register. After comparison with the actual signal, if the analog signal value is greater than the compare value, the result register&apos;s FCM bit will be set to high, else it will set low.&lt;/LI&gt;"/>
                        <p1:GCombo mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="ADC_SCAN_APP&#13;&#10;ADC_QUEUE_APP" value="ADC_SCAN_APP" widgetName="gcombo_req_src_selection" bounds="89, 113, 125, 21" toolTipText="Select either scan or queue request source for conversion." description="Select the required source APP needed for the conversion of the channels.&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt; Uses queue request source, if ADC_QUEUE_APP is selected &lt;/li&gt;&#13;&#10;&lt;li&gt; Uses scan request source, if ADC_SCAN_APP is selected &lt;/li&gt;&#13;&#10;&lt;li&gt; Queue request source enables flexible sequence of conversion. &lt;/li&gt;&#13;&#10;&lt;li&gt; Scan request source uses a fixed sequence of conversion (Always from higher channel number to a lower channel number). &lt;/li&gt;&#13;&#10;&lt;/ul&gt;"/>
                        <p1:GCheck manifestObj="true" text="Start after initialization" widgetName="gcheck_insert_at_init" bounds="76, 395, 180, 16" toolTipText="This enables the ADC after initialization; the start of conversion is configured by the trigger options." description="Insert the channels into the H/W registers.&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;QUEUE sequencer with wait for trigger: Starts the conversion of the channel on a trigger event. Trigger event is generated either by an external trigger signal or by calling ADC_MEASUREMENT_ADV_StartConversion().&lt;/li&gt;&#13;&#10;&lt;li&gt;QUEUE sequencer without wait for trigger: Starts conversion as soon as the channel is inserted into the queue buffer.&lt;/li&gt;&#13;&#10;&lt;li&gt;SCAN request source with external trigger: Starts conversion on a trigger event.  Trigger event is generated either by a external trigger signal or by calling ADC_MEASUREMENT_ADV_StartConversion().&lt;/li&gt;&#13;&#10;&lt;li&gt;SCAN request source without external trigger: Starts conversion on a ADC_MEASUREMENT_ADV_StartConversion().&#13;&#10;&lt;/ul&gt;&#13;&#10;&lt;br&gt;&#13;&#10;&lt;b&gt;Note:&lt;/b&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt; For Queue request source, this will insert the channels into the GxQINR0 register. &lt;/li&gt;&#13;&#10;&lt;li&gt; For Scan request source, this will insert the channels into the GxASSEL register. &lt;/li&gt;&#13;&#10;&lt;/ul&gt;&#13;&#10;" value="true"/>
                        <p1:GInteger x:Style="NONE" minValue="1" maxValue="8" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" manifestObj="true" widgetName="gfloat_no_of_channels" bounds="197, 65, 77, 23" value="1" toolTipText="Configure the required number of channels for conversion." description="Specify number of analog input channels to be measured.&#13;&#10;&lt;UL&gt;&#13;&#10;&lt;LI&gt;The maximum value will depend on the available number of channels for the given device &lt;/LI&gt;"/>
                        <p1:GInteger x:Style="NONE" minValue="0" maxValue="4" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" manifestObj="true" widgetName="ginteger_num_cev" enabled="false" value="0" bounds="569, 84, 25, 23" description="Shows the number of channel events configured in this APP (non-editable)."/>
                        <p1:GInteger bounds="480, 310, 25, 23" enabled="false" format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="32" minValue="0" value="0" widgetName="ginteger_num_sync_slaves" description="Shows the number of Sync slave channels configured in in this APP (non-editable)."/>
                        <p1:GInteger bounds="571, 204, 25, 23" enabled="false" format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="8" minValue="0" value="0" widgetName="ginteger_num_rev" description="Shows the number of result events configured in this APP (non-editable)."/>
                        <p1:GInteger bounds="571, 288, 25, 23" enabled="false" format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="8" minValue="0" value="0" widgetName="ginteger_num_src_event" description="Shows the number of the channels which will request for a source event. Configured in this APP (non-editable).&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt; In Queue the max value is 8 because all the channels can request a source event.&lt;/li&gt;&#13;&#10;&lt;li&gt; In Scan the max value is 1 because only after the last channel a source event occurs.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;"/>
                        <p1:GCheck manifestObj="true" widgetName="gcheck_expose_queue_scan" bounds="97, 138, 16, 16" toolTipText="Exposes the request source APP." description="Expose the request source APP. Each instance of the APP requests whether it should be connected to an request source APP or to consume the request source internally. If enabled the request source APP (ADC_SCAN/ ADC_QUEUE) will be added to the project. &lt;BR&gt;&#13;&#10;This feature enables sharing of the same request source between different APPS.&#13;&#10;"/>
                    </p1:GImage>
                    <p1:GLabel text="Note: &#13;&#10;Configure &apos;Trigger edge selection&apos; and &apos;Gating selection&apos; in ADC_SCAN / ADC_QUEUE APP." visible="true" widgetName="glabel_trigger_note" bounds="83, 433, 540, 40" manifestObj="true"/>
